    "@dynatrace-sdk/client-query": "^1.18.1",
    "@dynatrace-sdk/shared-errors": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "dotenv": "^16.4.7",
    "dt-app": "^0.140.1",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "jest": "^30.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
