{
  "name": "@theharithsa/dynatrace-mcp-server",
  "version": "2.6.0",
  "mcpName": "io.github.theharithsa/dynatrace-mcp-otel",
  "description": "Dynatrace Model Context Protocol (MCP) Server for AI assistants (Enhanced Fork)",
  "main": "./dist/index.js",
  "type": "commonjs",
  "bin": {
    "dynatrace-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "prompt.md",
    "SkillManifest.md",
    ".env.template",
    "dynatrace-agent-rules/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "dynatrace",
    "mcp",
    "model-context-protocol",
    "ai",
    "observability",
    "monitoring",
    "server"
  ],
  "author": "Vishruth Harithsa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theharithsa/dynatrace-mcp-otel.git"
  },
  "homepage": "https://github.com/theharithsa/dynatrace-mcp-otel#readme",
  "bugs": {
    "url": "https://github.com/theharithsa/dynatrace-mcp-otel/issues"
  },
  "directories": {
    "dist": "dist",
    "rules": "dynatrace-agent-rules"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc --build",
    "prepare": "husky",
    "watch": "tsc --watch",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration --runInBand",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "prettier --check . && tsc --noEmit",
    "lint:fix": "prettier --write .",
    "pre-commit": "npm run lint && npm run build && npm run test:unit",
    "release:prepare": "npm run lint && npm run build && npm run test",
    "start": "node dist/index.js",
    "dev": "npm run build && npm start",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@dynatrace-sdk/client-automation": "^5.3.0",
    "@dynatrace-sdk/client-classic-environment-v2": "^4.1.0",
    "@dynatrace-sdk/client-davis-copilot": "^1.0.0",
    "@dynatrace-sdk/client-document": "^1.24.5",
    "@dynatrace-sdk/client-platform-management-service": "^1.6.3",
    "@dynatrace-sdk/client-query": "^1.18.1",
    "@dynatrace-sdk/http-client": "^1.3.1",
    "@dynatrace-sdk/shared-errors": "^1.0.0",
    "@dynatrace/openkit-js": "^4.1.0",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.203.0",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@theharithsa/opentelemetry-instrumentation-mcp": "^1.0.0",
    "axios": "^1.12.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "dt-app": "^0.148.1",
    "form-data": "^4.0.4",
    "node-fetch": "^3.3.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
