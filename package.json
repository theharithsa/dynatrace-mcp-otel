  "description": "Dynatrace Model Context Protocol (MCP) Server for AI assistants",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "prompt.md",
