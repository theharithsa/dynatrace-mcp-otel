{
  "name": "@dynatrace-oss/dynatrace-mcp-server",
  "version": "0.4.0",
  "description": "Model Context Protocol (MCP) server for Dynatrace",
  "keywords": [
    "Dynatrace",
    "mcp",
    "model-context-protocol",
    "server"
  ],
  "main": "./dist/index.js",
  "type": "commonjs",
  "bin": {
    "mcp-server-dynatrace": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "bugs": {
    "url": "https://github.com/dynatrace-oss/dynatrace-mcp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dynatrace-oss/dynatrace-mcp.git"
  },
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "build": "tsc --build",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "test": "jest",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "author": "Dynatrace",
  "license": "MIT",
  "dependencies": {
    "@dynatrace-sdk/client-automation": "^5.3.0",
    "@dynatrace-sdk/client-classic-environment-v2": "^3.6.8",
